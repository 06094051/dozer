<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <configuration>
    <stop-on-errors>true</stop-on-errors>
    <date-format>MM/dd/yyyy HH:mm</date-format>
    <wildcard>false</wildcard>
    <bean-factory>net.sf.dozer.functional_tests.support.SampleDefaultBeanFactory</bean-factory>
  </configuration>

  <mapping>
    <class-a>net.sf.dozer.vo.AnotherTestObject</class-a>
    <class-b>net.sf.dozer.vo.AnotherTestObjectPrime</class-b>
    <field>
      <a>field4</a>
      <b>to.one</b>
    </field>
  </mapping>

  <mapping>
    <class-a>net.sf.dozer.vo.TestObject</class-a>
    <class-b>net.sf.dozer.vo.TestObjectPrime</class-b>
    <field>
      <a>one</a>
      <b>onePrime</b>
    </field>
  </mapping>

</mappings>