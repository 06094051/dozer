<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mappings PUBLIC "-//DOZER//DTD MAPPINGS//EN"
   "http://dozer.sourceforge.net/dtd/dozerbeanmapping.dtd">
<mappings>

  <configuration>
    <custom-converters>
      <converter type="net.sf.dozer.functional_tests.support.StringAppendCustomConverter">
        <class-a>java.lang.String</class-a>
        <class-b>java.lang.String</class-b>
      </converter>
      <converter type="net.sf.dozer.functional_tests.support.InjectedCustomConverter">
        <class-a>net.sf.dozer.vo.Vehicle</class-a>
        <class-b>net.sf.dozer.vo.Vehicle</class-b>
      </converter>
    </custom-converters>
  </configuration>

  <mapping>
    <class-a>net.sf.dozer.vo.SimpleObj</class-a>
    <class-b>net.sf.dozer.vo.SimpleObjPrime2</class-b>
    <field>
      <a>field1</a>
      <b>field1Prime</b>
    </field>
  </mapping>

</mappings>